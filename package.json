    "dev": "npm run build:content && vite",
    "test": "jest"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "js-yaml": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.6",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/topojson-client": "^3.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "qrcode": "^1.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
