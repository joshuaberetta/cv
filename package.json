{
  "name": "cv-generator",
  "version": "1.0.0",
  "description": "A modern CV generator using TypeScript and React",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:content && npm run build:pdf && tsc && vite build",
    "build:content": "ts-node scripts/build-globe-content.ts",
